<?xml version='1.0' encoding='utf-8'?>
<widget id="org.mozilla.beaker" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="WebSocket">
        <param name="android-package" value="com.knowledgecode.cordova.websocket.WebSocket" />
    </feature>
    <preference name="android-minSdkVersion" value="10" />
    <preference name="android-targetSdkVersion" value="17" />
    <icon density="mdpi" src="www/res/android/icon_mdpi.png" />
    <icon density="hdpi" src="www/res/android/icon_hdpi.png" />
    <icon density="xhdpi" src="www/res/android/icon_xhdpi.png" />
    <icon density="xxhdpi" src="www/res/android/icon_xxhdpi.png" />
    <icon density="xxxhdpi" src="www/res/android/icon_xxxhdpi.png" />
    <name>Webmaker</name>
    <description />
    <author href="https://www.mozilla.org">Mozilla Foundation</author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="www/res/icon.png" />
</widget>
